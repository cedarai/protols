{
	"comments": {
		"lineComment": "//",
		"blockComment": [ "/*", "*/" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["<", ">"]
	],
	"autoClosingPairs": [
		{ "open": "{", "close": "}" },
		{ "open": "[", "close": "]" },
		{ "open": "(", "close": ")" },
		{ "open": "<", "close": ">" },
		{ "open": "\"", "close": "\"", "notIn": ["string"] },
		{ "open": "'", "close": "'", "notIn": ["string"] },
		{ "open": "/*", "close": "*/", "notIn": ["string"] },
		{ "open": "//", "notIn": ["string", "comment"] }
	],
	"surroundingPairs": [
		{ "open": "\"", "close": "\"", "notIn": ["string"] }
	]
}
